<%- include ('./partials/header'); -%>
<div class="container">
        <div class="row">
            <div style="width: 30%; margin: 50px auto;">
                <form action="/edit?_method=PUT" method="POST">
                    <div class="form-group">
                            <input class="form-control" type="text" name="ticketId" placeholder="TicketId">
                        </div>
                    <div class="form-group">
                        <input class="form-control" type="time" name="time" min="00:00" max="23:59" placeholder="time">
                    </div>
                    <div class="form-group">
                        <input type="date" name="date" value="2020-08-30" min="2020-08-30">
                    </div>
                    <div class="form-group">
                        <input class="btn btn-lg btn-primary btn-block" type="submit"></input>
                    </div>
                </form>
            </div>
        </div>
    </div>
<%- include ('./partials/footer'); -%>
